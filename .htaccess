<IfModule mod_rewrite.c>
   RewriteEngine On
   # Force SSL
   RewriteCond %{HTTP_HOST} ^merlplus.bookingkh.com [NC]
   RewriteCond %{SERVER_PORT} 80
   RewriteRule ^(.*)$ https://merlplus.bookingkh.com/$1 [R,L]
   # Remove public folder form URL
   RewriteRule ^(.*)$ public/$1 [L]
   #Libwww-perl Access
   RewriteCond %{HTTP_USER_AGENT} libwww-perl.*
   RewriteRule .* ? [F,L]
   #URL Canonicalization
   RewriteCond %{HTTP_HOST} !^merlplus.bookingkh.com$ [NC]
   RewriteRule ^(.*)$ https://merlplus.bookingkh.com/$1 [L,R=301]
    #IP Canonicalization
   RewriteCond %{HTTP_HOST} ^139\.162\.13\.242
   RewriteRule (.*) https://merlplus.bookingkh.com/$1 [R=301,L]
</IfModule>

<IfModule mod_expires.c>
    ExpiresActive On
    ExpiresByType image/gif A2592000
    ExpiresByType image/png A2592000
    ExpiresByType image/jpg A2592000
    ExpiresByType image/jpeg A2592000
</IfModule>